#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangzihan/Code/GeoScratch/node_modules/.pnpm/npm@11.5.2/node_modules/npm/bin/node_modules:/Users/zhangzihan/Code/GeoScratch/node_modules/.pnpm/npm@11.5.2/node_modules/npm/node_modules:/Users/zhangzihan/Code/GeoScratch/node_modules/.pnpm/npm@11.5.2/node_modules:/Users/zhangzihan/Code/GeoScratch/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhangzihan/Code/GeoScratch/node_modules/.pnpm/npm@11.5.2/node_modules/npm/bin/node_modules:/Users/zhangzihan/Code/GeoScratch/node_modules/.pnpm/npm@11.5.2/node_modules/npm/node_modules:/Users/zhangzihan/Code/GeoScratch/node_modules/.pnpm/npm@11.5.2/node_modules:/Users/zhangzihan/Code/GeoScratch/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/npx-cli.js" "$@"
else
  exec node  "$basedir/../../bin/npx-cli.js" "$@"
fi
